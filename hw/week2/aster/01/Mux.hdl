/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=Notsel);
    And(a=a, b=Notsel, out=out1);
    And(a=b, b=sel, out=out2);
    Or(a=out1, b=out2, out=out);
    //// Replace this comment with your code.
}